class Hola {
    init(saludo) {
        this.saludo = saludo;
    }
    class statMethod(a) {
        print a;
    }

    class statMethod2(a) {
        print "2" + a;
    }

    normalMethod() {
        print this.saludo;
    }
}

var sa = Hola("hola");
Hola.statMethod("hola metodo estatico");
sa.normalMethod();
Hola.statMethod("llamando metodo estatico desde instancia");
Hola.statMethod2("llamando metodo estatico nro 2 desde instancia");
