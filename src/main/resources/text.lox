class Hola {
    class esPrimo(number) {
        if (number == 1) return false;

        for (var i = 2; i < number; i = i + 1) {
            if ((number % i) == 0) {
               return false;
            }
        }
        return true;
    }
}


var number = 100;

print Hola.esPrimo(number) ? (number + " es primo.") : (number + " no es primo.");